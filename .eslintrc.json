{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "linebreak-style": ["error", "unix"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
  }
}